{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "recent-nft-sales",
  "title": "Recent NFT Sales",
  "description": "Recent sales from NFT Contract or Name",
  "icon": "command-icon.png",
  "author": "tywenk",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-by-contract-address",
      "title": "Search by Contract Address",
      "description": "Enter a contract address and get recent sales of that item",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3",
    "@tanstack/react-query": "^4.0.10",
    "@zoralabs/zdk": "^2.1.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
